<!DOCTYPE html>
<html>
<head>
  <title>LibMash</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="flash">
        <%flash.each do|type,message| -%>
      <div class= "alert alert-<%= type %>"><%= message %></div>
        <%end %>
  </div>
  <div class="container">

    <div class="jumbotron">
      <%= yield %>
    </div>
  </div>


</body>
</html>
